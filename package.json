{
  "name": "discord-bot-v3",
  "version": "1.0.0",
  "description": "discord bot for music and more",
  "author": "Yamami1221",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint --ext .ts src/",
    "start": "npx tsc && node build/main",
    "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --exec \"npx tsc && node build/main\"",
    "prod": "node build/main"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@typescript-eslint/parser": "^7.9.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.2.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.9.0"
  },
  "dependencies": {
    "@discord-player/extractor": "^4.4.7",
    "@evan/opus": "^1.0.3",
    "discord-player": "^6.6.8",
    "discord.js": "^14.15.2",
    "dotenv": "^16.4.5",
    "ffmpeg-static": "^5.2.0",
    "mediaplex": "^0.0.9",
    "mongodb": "^6.6.1",
    "ytdl-core": "^4.11.5"
  }
}
